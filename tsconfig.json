{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": ["nativewind/types"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".nativewind/types/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
